    "description": "PHP wrapper for Cronofy's unified calendar API",
    "version": "v1.1.10",
    "autoload": {
        "files": [
            "src/Cronofy.php"
        ],
        "psr-4": {
            "Cronofy\\": "src/"
        }
    },
    "keywords": [
